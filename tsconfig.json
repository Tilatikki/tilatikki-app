{
  "compilerOptions": {
    "target": "es2016",                                 


    /* Modules */
    "module": "commonjs",                                
    "rootDir": "./server",                                  
     "outDir": "./build",                                 
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,    
    "typeRoots": ["./global.d.ts", "./node_modules/@types"],      

    /* Type Checking */
    "strict": true,                                      
    "skipLibCheck": true                                
  },
  "exclude": ["client",".env"]
}